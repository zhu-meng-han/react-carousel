		"build": "node build/build.js",
		"prod": "node build/build.js",
		"analyzer": "node build/analyzer.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "lint-staged",
			"commit-msg": "node build/verifyCommitMsg.js"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"src/**/*.{css,scss}": [
			"prettier --write",
			"stylelint --fix --syntax scss",
			"git add"
		],
		"src/**/*.{md,json}": [
			"prettier --write",
			"git add"
		]
	},
